{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "db:setup": "medusa db:setup --db $DB_NAME",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "inspect": "node --inspect ./node_modules/@medusajs/medusa/dist/index.js",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:generate": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts",
    "test:generate:watch": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --watch",
    "test:generate:watchAll": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --watchAll",
    "test:generate:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --coverage",
    "test:generate:keep": "KEEP_TEST_OUTPUT=1 NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts",
    "test:generate:keep:watchAll": "KEEP_TEST_OUTPUT=1 NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --watchAll",
    "test:generate:keep:quiet:watchAll": "KEEP_TEST_OUTPUT=1 NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --silent --noStackTrace --json --outputFile=.test-results.json --watchAll && node scripts/process-test-titles.js",
    "test:generate:quiet": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --silent --noStackTrace --json --outputFile=.test-results.json && node scripts/process-test-titles.js",
    "test:generate:quiet:fileonly": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --silent --noStackTrace --json --outputFile=.test-results.json > /dev/null 2>&1",
    "test:generate:single": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --silent --noStackTrace",
    "test:generate:titles": "NODE_OPTIONS=--experimental-vm-modules jest --config=scripts/module-generator/tests/jest.config.ts --silent --noStackTrace --json --outputFile=.test-results.json && node scripts/process-test-titles.js",
    "src:generate": "NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" ts-node scripts/module-generator/src/cli.ts",
    "src:generate:debug": "DEBUG=1 NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" ts-node scripts/module-generator/src/cli.ts",
    "src:generate:dry-run": "DEBUG=1 DRY_RUN=1 NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" ts-node scripts/module-generator/src/cli.ts",
    "src:generate:watch": "NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" ts-node scripts/module-generator/src/utils/watch.ts scripts/module-generator/configs/production-modules.ts"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.6.1",
    "@medusajs/cli": "2.6.1",
    "@medusajs/framework": "2.6.1",
    "@medusajs/medusa": "2.6.1",
    "@mikro-orm/core": "6.4.9",
    "@mikro-orm/knex": "6.4.10",
    "@mikro-orm/migrations": "6.4.9",
    "@mikro-orm/postgresql": "6.4.9",
    "awilix": "^12.0.5",
    "pg": "^8.14.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@medusajs/test-utils": "2.6.1",
    "@mikro-orm/cli": "6.4.9",
    "@swc/core": "1.11.9",
    "@swc/jest": "^0.2.37",
    "@types/glob": "^8.1.0",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "chalk": "^5.4.1",
    "glob": "^11.0.1",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "jest-raw-loader": "^1.0.1",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "yalc": "1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
