import { model } from "@medusajs/framework/utils";
{{#each model.relations}}
import {{this.model}} from "./{{toKebabCase this.model}}";
{{/each}}

const {{toPascalCase model.name}} = model.define("{{toSnakeCase model.name}}", {
  id: model.id().primaryKey(),
  {{#each model.fields}}
  {{{processField this}}}{{#unless @last}},{{/unless}}
  {{/each}}
});

export default {{toPascalCase model.name}};